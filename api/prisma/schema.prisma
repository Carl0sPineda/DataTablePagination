generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id           String  @id @default(uuid())
  name         String
  price        Float
  brand        String
  model_year   String
  quantity     Int
  color        String
  availability Boolean @default(true)

  @@map(name: "products")
}
